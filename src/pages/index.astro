---
const title = '';

import astroConfig from '@config';
import Layout from '@layout/Default.astro';

import Breadcrumb from '@component/uswds/Breadcrumb.astro';

const breadcrumbs = [
{
		title: "this is a page",
		link: "./page-link",
		current: false,
	},
	{
		title: "this is another page",
		link: "./page-link-2",
		current: false,
	},
	{
		title: "this is a third page",
		current: true,
	},
];
---

<Layout title={title}>
	This is content!

	<Breadcrumb items={breadcrumbs}/>
</Layout>
